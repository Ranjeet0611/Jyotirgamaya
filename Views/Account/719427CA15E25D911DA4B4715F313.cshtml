    <!-- Header -->
    <div class="header bg-gradient-primary py-7 py-lg-8">
      <div class="container">
        <div class="header-body text-center mb-7">
          <div class="row justify-content-center">
            <div class="col-lg-5 col-md-6">
              <h1 class="text-white">Welcome!</h1>
              <p class="text-lead text-light"></p>
            </div>
          </div>
        </div>
      </div>
      <div class="separator separator-bottom separator-skew zindex-100">
        <svg x="0" y="0" viewBox="0 0 2560 100" preserveAspectRatio="none" version="1.1" xmlns="http://www.w3.org/2000/svg">
          <polygon class="fill-default" points="2560 0 2560 100 0 100"></polygon>
        </svg>
      </div>
    </div>
    <!-- Page content -->
    <div class="container mt--8 pb-5">
      <!-- Table -->
      <div class="row justify-content-center">
        <div class="col-lg-6 col-md-8">
          <div class="card bg-secondary shadow border-0">
             
           
            <div class="card-body px-lg-5 py-lg-5" style="background-color:white;border-radius:8px;">
                 <center><img src="~/images/download1.png" style="width:180px;height:150px;"/></center>
                 <br />
                @if (TempData["Meesage"] == "Error")
                {
                    <center><h3 class="text-danger">This account has already been added</h3></center>
                }
                @if (TempData["Meesage"] == "Error1")
                {
                    <center><h3 class="text-danger">Something Went wrong we will look into it</h3></center>
                }
                @if (TempData["Message"] == "Success")
                {
                    <center><h3 class="text-success">Your Account has been Created</h3></center>
                }
                
              <div class="text-center text-muted mb-4">
                <h5 class="text-lead">Sign up with credentials</h5>
              </div>
              @using (Html.BeginForm("Register", "Account", FormMethod.Post, new { enctype = "multipart/form-data" }))
              {
                  @Html.AntiForgeryToken()
                <div class="form-group">
                  <div class="input-group input-group-alternative mb-3">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-user-circle-o" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    @*<input class="form-control" placeholder="Name" type="text">*@
                      @Html.TextBoxFor(model => model.FirstName, new { @class = "form-control", @placeholder = "Enter your First Name",@minlength="3",@required="required" })
                      @Html.ValidationMessageFor(Model => Model.FirstName, "", new { @class = "text-danger" })
                  </div>
                </div>
                <div class="form-group">
                  <div class="input-group input-group-alternative mb-3">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-user-circle-o" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    @*<input class="form-control" placeholder="Email" type="email">*@
                      @Html.TextBoxFor(model => model.LastName, new { @class = "form-control", @placeholder = "Enter your Last Name",@minlength="3",@required="required" })
                      @Html.ValidationMessageFor(Model => Model.LastName, "", new { @class = "text-danger" })
                  </div>
                </div>
                <div class="form-group">
                  <div class="input-group input-group-alternative">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-unlock-alt" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    @*<input class="form-control" placeholder="Password" type="password">*@
                      @Html.PasswordFor(model => model.Password, new { @class = "form-control", @placeholder = "Enter your Password",@minlength="8",@required="required" })
                      @Html.ValidationMessageFor(Model => Model.Password, "", new { @class = "text-danger" })
                  </div>
                </div>
                <div class="form-group" id="FormEmail">
                  <div class="input-group input-group-alternative">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-envelope-o" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    <input class="form-control" data-val="true" data-val-required="Enter your Email" id="Email" name="Email" placeholder="Enter your Email" required="required" type="email" value="" />
                      @*@Html.TextBoxFor(model => model.Email, new { @class = "form-control", @placeholder = "Enter your Email",required="required" })*@
                      @Html.ValidationMessageFor(Model => Model.Email, "", new { @class = "text-danger" })
                  </div>
                </div>
                  <div class="form-group">
                  <div class="input-group input-group-alternative">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-mobile" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    @*<input class="form-control" placeholder="Password" type="password">*@
                      @Html.TextBoxFor(model => model.MobileNumber, new { @class = "form-control", @placeholder = "Enter your Mobile Number",@minlength="10",@maxlength="10",@required="required" })
                      @Html.ValidationMessageFor(model => model.MobileNumber, "", new { @class = "text-danger" })
                  </div>
                </div>
                  <div class="form-group">
                  <div class="input-group input-group-alternative">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-globe" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    @*<input class="form-control" placeholder="Password" type="password">*@
                      @Html.TextBoxFor(model => model.City, new { @class = "form-control", @placeholder = "Enter your City",@required="required" })
                      @Html.ValidationMessageFor(model=>model.City, "", new { @class = "text-danger" })
                  </div>
                </div>
                  <div class="form-group">
                  <div class="input-group input-group-alternative">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-pencil" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    @*<input class="form-control" placeholder="Password" type="password">*@
                      @Html.TextBoxFor(model => model.Pincode, new { @class = "form-control", @placeholder = "Enter your City Pincode",@required="required" })
                      @Html.ValidationMessageFor(model=>model.Pincode, "", new { @class = "text-danger" })
                  </div>
                </div>
                  
                 <div class="form-group">
                  <div class="input-group input-group-alternative">
                    <div class="input-group-prepend">
                      <span class="input-group-text"><i class="fa fa-pencil" aria-hidden="true" style="color:black;"></i></span>
                    </div>
                    @*<input class="form-control" placeholder="Password" type="password">*@
                      @Html.TextBoxFor(model => model.SecurityName, new { @class = "form-control", @placeholder = "Enter Security Name",@required="required" })
                      @Html.ValidationMessageFor(model=>model.SecurityName, "", new { @class = "text-danger" })
                  </div>
                     <br />
                     <h5 class="text-lead">Security Name helps you to recover your password</h5>
                </div>
                 
                @*<div class="text-muted font-italic"><small>password strength: <span class="text-success font-weight-700">strong</span></small></div>*@
                <div class="row my-4">
                  <div class="col-12">
                    <div class="custom-control custom-control-alternative custom-checkbox">
                      <input class="custom-control-input" id="customCheckRegister" type="checkbox" required>
                      <label class="custom-control-label" for="customCheckRegister">
                        <span class="text-muted">I agree with the <a href="#!">Privacy Policy</a></span>
                      </label>
                    </div>
                  </div>
                </div>
                <div class="text-center">
                  <input type="submit" class="btn btn-primary mt-4" value="Create Account">
                </div>
              }
            </div>
          </div>
        </div>
      </div>
    </div>
